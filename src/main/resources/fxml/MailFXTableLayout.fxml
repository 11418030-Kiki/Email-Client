<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.brandonbalala.controllers.MailFXTableLayoutController">
   <children>
      <TableView fx:id="mailDataTable" editable="true" onDragDetected="#dragDetected" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columns>
            <TableColumn fx:id="toColumn" prefWidth="75.0" text="%TO" />
            <TableColumn fx:id="subjectColumn" prefWidth="75.0" text="%SUBJECT" />
            <TableColumn fx:id="dateReceivedColumn" prefWidth="75.0" text="%DATERECEIVED" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
